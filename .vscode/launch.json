{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch ${project.name}",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net6.0/dotnettest.dll",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}